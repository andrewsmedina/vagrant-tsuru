listen: <%= node[:tsuru][:webserver] %>
use-tls: <%= node[:tsuru][:use_tls] %>
#tls-cert-file: /path/to/cert.pem
#tls-key-file: /path/to/key.pem
database:
  url: <%= node[:tsuru][:db_url] %>
  name: <%= node[:tsuru][:tsuru_db_name] %>
git:
  unit-repo: /home/git/repositories
  host: 127.0.0.1
  protocol: http
  port: <%= node[:tsuru][:gandalf_web_port] %>
bucket-support: true
aws:
  access-key-id: access-id-here
  secret-access-key: secret-very-secret
  iam:
    endpoint: https://iam.amazonaws.com/
  s3:
    region-name: us-west-1
    endpoint: https://ec2.us-west-1.amazonaws.com
    bucketEndpoint: https://s3-us-west-1.amazonaws.com
    location-constraint: true
    lowercase-bucket: true
auth:
  salt: tsuru-salt
  token-expire-days: 2
  token-key: TSURU-KEY
juju:
  charms-path: /home/git/charms
  units-collection: juju_units
  elb-collection: juju_load_balancers
  use-elb: false
  elb-endpoint: https://elasticloadbalancing.amazonaws.com
  elb-avail-zones:
    - us-west-1a
queue-server: "127.0.0.1:11300"
admin-team: admin
smtp: 
  host: smtp.example.com
  port: 25
  user: admin@example.com
  password: 123456
